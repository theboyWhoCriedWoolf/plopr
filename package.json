{
  "name": "plopr",
  "version": "1.0.0",
  "license": "MIT",
  "author": "Tal Woolf",
  "repository": {
    "type": "git",
    "url": "git://github.com/theboyWhoCriedWoolf/plopr.git"
  },
  "bugs": {
    "url": "https://github.com/theboyWhoCriedWoolf/plopr/issues"
  },
  "engines": {
    "npm": ">=5",
    "node": ">=8.10.0"
  },
  "bin": {
    "plopr": "./bin/plop-scripts.js"
  },
  "files": [
    "bin",
    "generators"
  ],
  "scripts": {
    "precommit": "npm run lint",
    "commit": "npx git-cz",
    "lint": "eslint ./generators --fix",
    "generate": "plop --plopfile ./generators/index.js --freestyle"
  },
  "dependencies": {
    "cross-spawn": "^6.0.5",
    "inquirer-directory": "^2.1.0",
    "plop": "^2.1.0",
    "prettier": "^1.15.3",
    "yargs": "^12.0.5"
  },
  "peerDependencies": {
    "react": "^16.7.0-alpha.2",
    "eslint": "^5.10.0"
  },
  "devDependencies": {
    "eslint": "^5.10.0",
    "babel-eslint": "^10.0.1",
    "eslint-config-prettier": "^3.3.0",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-jsx-a11y": "^6.1.2",
    "eslint-plugin-prettier": "^3.0.0",
    "eslint-plugin-react": "^7.11.1"
  },
  "tags": [
    "React",
    "plop",
    "generators",
    "components"
  ],
  "keywords": [
    "plop generators",
    "React Hooks",
    "React generators"
  ]
}
